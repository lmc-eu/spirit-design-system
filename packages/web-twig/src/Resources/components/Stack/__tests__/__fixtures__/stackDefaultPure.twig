{% embed "@spirit/stack.twig" %}
    {% block content %}
        <div>Block 1</div>
        <div>Block 2</div>
        <div>Block 3</div>
    {% endblock %}
{% endembed %}

{% embed "@spirit/stack.twig" with { props: {
    elementType: 'ul'
}} %}
    {% block content %}
        <li>List item 1</li>
        <li>List item 2</li>
        <li>List item 3</li>
    {% endblock %}
{% endembed %}
