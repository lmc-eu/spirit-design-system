{% set colors = [ "primary", "secondary", "inverted" ] %}

<section class="docs-Section">

    <h2 class="docs-Heading">Colors</h2>

    {% for color in colors %}
        <p{% if color is same as('inverted') %} class="docs-Box"{% endif %}>
            <Link href="#" color={color}>Link {{ color }}</Link>
        </p>
    {% endfor %}
</section>
