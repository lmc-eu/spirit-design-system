<!-- Render as div -->
<Heading elementType="div">
    Example heading
</Heading>

<!-- Render as h1 -->
<Heading elementType="h1">
    Example heading
</Heading>

<!-- Render as h1 semibold -->
<Heading elementType="h1" emphasis="semibold">
    Example heading
</Heading>

<!-- Render with all props -->
<Heading
    elementType="h2"
    emphasis="italic"
    size="large"
    translate="no"
>
    Example heading
</Heading>
