  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "babel-loader": "8.2.3",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.2"
  }
}
